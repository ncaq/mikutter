#: ../activity.rb:47
msgid "種類"
msgstr "種類"

#: ../activity.rb:48
msgid "説明"
msgstr "說明"

#: ../activity.rb:49
msgid "時刻"
msgstr "時間"

#: ../activity.rb:166 ../activity.rb:315 ../activity.rb:318
msgid "アクティビティ"
msgstr "活動"

#: ../activity.rb:222
msgid "@%{user} がふぁぼふぁぼしました"
msgstr "@%{user} 喜歡這則推"

#: ../activity.rb:232
msgid "@%{user} があんふぁぼしました"
msgstr "@%{user} 不喜歡這則推了"

#: ../activity.rb:244
msgid "@%{user} がリツイートしました"
msgstr "@%{user} RT了這則推"

#: ../activity.rb:250
msgid "リツイートソースが取得できませんでした"
msgstr "RT來源取得失敗"

#: ../activity.rb:254
msgid "@%{user}が%{list}に追加されました"
msgstr "@%{user} 被加入 %{list} 列表中"

#: ../activity.rb:262 ../activity.rb:279
msgid "%{description} (by @%{user})"
msgstr "%{description} (來自 @%{user})"

#: ../activity.rb:271
msgid "@%{user}が%{list}から削除されました"
msgstr "@%{user} 被踢出 %{list} 了"

#: ../activity.rb:291
msgid "@%{followee} が @%{follower} をﾌｮﾛｰしました"
msgstr "@%{followee} 關注了 @%{follower}"

#: ../activity.rb:303
msgid "差出人: @%{sender}"
msgstr "發訊者: @%{sender}"

#: ../activity.rb:304
msgid "宛先: @%{recipient}"
msgstr "收訊者: @%{recipient}"

#: ../activity.rb:335
msgid "%{name}を表示する"
msgstr "顯示 %{name}"

#: ../activity.rb:335
msgid "%{name}を、アクティビティタイムラインに表示します。チェックを外すと、%{name}の他の設定は無効になります。"
msgstr "在活動時間軸中顯示%{name}。若取消勾選，%{name}的其他設定皆不會生效。"

#: ../activity.rb:336
msgid "自分に関係ない%{name}も表示する"
msgstr "顯示與自己無關的%{name}"

#: ../activity.rb:336
msgid ""
"自分に関係ない%{name}もアクティビティタイムラインに表示されるようになります。チェックを外すと、自分に関係ない%{name}は表示されません。"
msgstr "與自己無關的%{name}也會顯示在活動時間軸中。\n若取消勾選，則只會顯示與自己相關的%{name}。"

#: ../activity.rb:337
msgid "タイムラインに表示"
msgstr "顯示在時間軸"

#: ../activity.rb:337
msgid "%{name}が通知された時に、システムメッセージで%{name}を通知します"
msgstr "收到%{name}的通知時，以系統訊息通知%{name}"

#: ../activity.rb:338
msgid "ステータスバーに表示"
msgstr "顯示在狀態欄"

#: ../activity.rb:338
msgid "%{name}が通知された時に、ステータスバーにしばらく表示します"
msgstr "收到%{name}的通知時，狀態欄中會出現暫時性的訊息"

#: ../activity.rb:343
msgid "リツイート"
msgstr "轉推"

#: ../activity.rb:344
msgid "ふぁぼ"
msgstr "喜歡"

#: ../activity.rb:345
msgid "フォロー"
msgstr "關注"

#: ../activity.rb:346
msgid "リストに追加"
msgstr "添加至列表"

#: ../activity.rb:347
msgid "リストから削除"
msgstr "從列表中刪除"

#: ../activity.rb:348
msgid "ダイレクトメッセージ"
msgstr "DM"

#: ../activity.rb:349
msgid "システムメッセージ"
msgstr "系統訊息"

#: ../activity.rb:350
msgid "エラー"
msgstr "錯誤訊息"
