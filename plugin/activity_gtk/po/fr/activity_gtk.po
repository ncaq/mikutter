#: ../activity.rb:47
msgid "種類"
msgstr "Type"

#: ../activity.rb:48
msgid "説明"
msgstr "Description"

#: ../activity.rb:49
msgid "時刻"
msgstr "Heure"

#: ../activity.rb:166 ../activity.rb:315 ../activity.rb:318
msgid "アクティビティ"
msgstr "Activités"

#: ../activity.rb:222
msgid "@%{user} がふぁぼふぁぼしました"
msgstr "@%{user} ajouté aux favoris"

#: ../activity.rb:232
msgid "@%{user} があんふぁぼしました"
msgstr "@%{user} retiré des favoris"

#: ../activity.rb:244
msgid "@%{user} がリツイートしました"
msgstr "@%{user} a retwitté"

#: ../activity.rb:250
msgid "リツイートソースが取得できませんでした"
msgstr "Echec à la reception de la source de retweet"

#: ../activity.rb:254
msgid "@%{user}が%{list}に追加されました"
msgstr "@%{user} a été ajouté à la liste %{list}"

#: ../activity.rb:262 ../activity.rb:279
msgid "%{description} (by @%{user})"
msgstr "%{description} (par @%{user})"

#: ../activity.rb:271
msgid "@%{user}が%{list}から削除されました"
msgstr "@%{user} a été enlevé de la liste %{list}"

#: ../activity.rb:291
msgid "@%{followee} が @%{follower} をﾌｮﾛｰしました"
msgstr "@%{followee} suit @%{follower}"

#: ../activity.rb:303
msgid "差出人: @%{sender}"
msgstr "Expéditeur: @%{sender}"

#: ../activity.rb:304
msgid "宛先: @%{recipient}"
msgstr "Destinataire: @%{recipient}"

#: ../activity.rb:335
msgid "%{name}を表示する"
msgstr "Afficher %{name}"

#: ../activity.rb:335
msgid "%{name}を、アクティビティタイムラインに表示します。チェックを外すと、%{name}の他の設定は無効になります。"
msgstr "Afficher %{name} dans le fil d'activité. Si non coché, les autres paramètres de la section de %{name} seront désactivés."

#: ../activity.rb:336
msgid "自分に関係ない%{name}も表示する"
msgstr "Afficher %{name} qui ne sont pas relatifs à moi"

#: ../activity.rb:336
msgid ""
"自分に関係ない%{name}もアクティビティタイムラインに表示されるようになります。チェックを外すと、自分に関係ない%{name}は表示されません。"
msgstr "Si coché, n'importe quel %{name} qui ne sont pas relatifs à moi s'afficheront dans le fil d'activité. Décocher pour désactiver."

#: ../activity.rb:337
msgid "タイムラインに表示"
msgstr "Afficher dans le fil d'actualité"

#: ../activity.rb:337
msgid "%{name}が通知された時に、システムメッセージで%{name}を通知します"
msgstr "Notifier %{name} en tant que message système."

#: ../activity.rb:338
msgid "ステータスバーに表示"
msgstr "Afficher dans la barre de status"

#: ../activity.rb:338
msgid "%{name}が通知された時に、ステータスバーにしばらく表示します"
msgstr "Lorsque vous recevez %{name}, le laisser s'afficher quelques secondes dans la barre de status."

#: ../activity.rb:343
msgid "リツイート"
msgstr "Retweets"

#: ../activity.rb:344
msgid "ふぁぼ"
msgstr "Favs."

#: ../activity.rb:345
msgid "フォロー"
msgstr ""

#: ../activity.rb:346
msgid "リストに追加"
msgstr "Ajouter à la liste"

#: ../activity.rb:347
msgid "リストから削除"
msgstr "Enlever de la liste"

#: ../activity.rb:348
msgid "ダイレクトメッセージ"
msgstr "Messages directs"

#: ../activity.rb:349
msgid "システムメッセージ"
msgstr "Messages système"

#: ../activity.rb:350
msgid "エラー"
msgstr "Erreurs"
